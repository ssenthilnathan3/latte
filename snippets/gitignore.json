{
  "frappe_gitignore": {
    "prefix": ["gitignore", "frappe_gitignore"],
    "body": [
      "# Byte-compiled / optimized / DLL files",
      "__pycache__/",
      "*.py[cod]",
      "*$py.class",
      "",
      "# C extensions",
      "*.so",
      "",
      "# Distribution / packaging",
      ".Python",
      "build/",
      "develop-eggs/",
      "dist/",
      "downloads/",
      "eggs/",
      ".eggs/",
      "lib/",
      "lib64/",
      "parts/",
      "sdist/",
      "var/",
      "wheels/",
      "*.egg-info/",
      ".installed.cfg",
      "*.egg",
      "",
      "# PyInstaller",
      "*.manifest",
      "*.spec",
      "",
      "# Installer logs",
      "pip-log.txt",
      "pip-delete-this-directory.txt",
      "",
      "# Unit test / coverage reports",
      "htmlcov/",
      ".tox/",
      ".coverage",
      ".coverage.*",
      ".cache",
      "nosetests.xml",
      "coverage.xml",
      "*.cover",
      ".hypothesis/",
      ".pytest_cache/",
      "",
      "# Translations",
      "*.mo",
      "*.pot",
      "",
      "# Django stuff:",
      "*.log",
      "local_settings.py",
      "db.sqlite3",
      "",
      "# Flask stuff:",
      "instance/",
      ".webassets-cache",
      "",
      "# Scrapy stuff:",
      ".scrapy",
      "",
      "# Sphinx documentation",
      "docs/_build/",
      "",
      "# PyBuilder",
      "target/",
      "",
      "# Jupyter Notebook",
      ".ipynb_checkpoints",
      "",
      "# pyenv",
      ".python-version",
      "",
      "# celery beat schedule file",
      "celerybeat-schedule",
      "",
      "# SageMath parsed files",
      "*.sage.py",
      "",
      "# Environments",
      ".env",
      ".venv",
      "env/",
      "venv/",
      "ENV/",
      "env.bak/",
      "venv.bak/",
      "",
      "# Spyder project settings",
      ".spyderproject",
      ".spyproject",
      "",
      "# Rope project settings",
      ".ropeproject",
      "",
      "# mkdocs documentation",
      "/site",
      "",
      "# mypy",
      ".mypy_cache/",
      "",
      "# Frappe",
      ".DS_Store",
      "*.pyc",
      "*.egg-info",
      "dist/",
      "build/",
      ".cache/",
      "",
      "# Sites",
      "sites/*/site_config.json",
      "sites/*/private/backups/*",
      "sites/*/private/files/*",
      "sites/*/public/files/*",
      "sites/*/logs/*",
      "sites/*/task-logs/*",
      "sites/assets/*",
      "",
      "# Node",
      "node_modules/",
      "*.min.js",
      "*.min.css",
      "",
      "# Production",
      "*.sock"
    ],
    "description": "Complete .gitignore for Frappe projects"
  },
  "frappe_app_gitignore": {
    "prefix": ["app_gitignore", "frappe_app_gitignore"],
    "body": [
      "# Byte-compiled / optimized / DLL files",
      "__pycache__/",
      "*.py[cod]",
      "*$py.class",
      "",
      "# Distribution / packaging",
      "*.egg-info/",
      "dist/",
      "build/",
      "",
      "# Unit test / coverage reports",
      "htmlcov/",
      ".coverage",
      ".pytest_cache/",
      "",
      "# Environment",
      ".env",
      "",
      "# IDE",
      ".vscode/",
      ".idea/",
      "*.swp",
      "*.swo",
      "",
      "# OS",
      ".DS_Store",
      "Thumbs.db",
      "",
      "# Node modules (if any)",
      "node_modules/",
      "",
      "# Compiled assets",
      "*.min.js",
      "*.min.css"
    ],
    "description": "Gitignore for individual Frappe apps"
  },
  "frappe_sites_gitignore": {
    "prefix": ["sites_gitignore", "frappe_sites_gitignore"],
    "body": [
      "# Site specific files that shouldn't be committed",
      "*/site_config.json",
      "*/site_config_dev.json",
      "*/site_config_staging.json",
      "",
      "# Private files",
      "*/private/backups/*",
      "*/private/files/*",
      "",
      "# Public files (user uploads)",
      "*/public/files/*",
      "",
      "# Logs",
      "*/logs/*",
      "*/task-logs/*",
      "",
      "# Assets (compiled)",
      "assets/*",
      "",
      "# Keep common site config",
      "!common_site_config.json",
      "",
      "# Keep directory structure",
      "!*/private/",
      "!*/public/",
      "!*/logs/",
      "!*/task-logs/"
    ],
    "description": "Gitignore for sites directory"
  },
  "frappe_docker_gitignore": {
    "prefix": ["docker_gitignore", "frappe_docker_gitignore"],
    "body": [
      "# Docker",
      ".docker/",
      "docker-compose.override.yml",
      "",
      "# Environment files",
      ".env",
      ".env.local",
      ".env.development",
      ".env.production",
      "",
      "# Volumes",
      "volumes/",
      "",
      "# SSL certificates",
      "*.pem",
      "*.crt",
      "*.key"
    ],
    "description": "Additional gitignore entries for Dockerized Frappe"
  }
}
